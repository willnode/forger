{
  "name": "@sveltejs/svelte-repl",
  "version": "0.4.1",
  "description": "The <Repl> component used on the Svelte website",
  "main": "src/Repl.svelte",
  "svelte": "src/Repl.svelte",
  "scripts": {
    "dev": "rollup -cw",
    "build": "rollup -c",
    "srcdoc:update": "curl https://raw.githubusercontent.com/sveltejs/template/master/public/global.css > src/Output/srcdoc/styles.css",
    "srcdoc": "npm run srcdoc:update && node scripts/build-srcdoc.js",
    "prepublishOnly": "npm run srcdoc && npm run build && npm test",
    "test": "echo \"no tests yet\"",
    "lint": "eslint src/**.svelte src/**.js",
    "postinstall": "patch-package"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.0.6",
    "eslint": "^6.6.0",
    "eslint-plugin-svelte3": "^2.7.3",
    "rollup": "^2.77.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-polyfill-node": "^0.10.1",
    "rollup-plugin-terser": "^5.3.0",
    "svelte": "^3.14.1"
  },
  "repository": "https://github.com/sveltejs/svelte-repl",
  "author": "Rich Harris",
  "license": "LIL",
  "keywords": [
    "svelte"
  ],
  "files": [
    "src",
    "workers"
  ],
  "dependencies": {
    "codemirror": "^5.49.2",
    "estree-walker": "^0.9.0",
    "events": "^3.3.0",
    "fs": "npm:memfs",
    "marked": "^4.0.0",
    "os": "npm:os-browserify",
    "patch-package": "^6.4.7",
    "path": "npm:path-browserify",
    "perf_hooks": "npm:universal-perf-hooks",
    "postcss": "^8.4.14",
    "postcss-import": "^14.1.0",
    "sass": "^1.54.0",
    "sourcemap-codec": "^1.4.6",
    "stream": "npm:stream-browserify",
    "svelte-json-tree": "0.0.5",
    "svelte-listener": "git+https://github.com/RedHatter/svelte-listener.git",
    "util": "^0.12.4",
    "vite": "npm:browser-vite",
    "yootils": "0.0.16"
  }
}
